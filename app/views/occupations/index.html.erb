<h3>Occupation list</h3>

<table>
    <tr>
        <th>occupation titles</th>
    </tr>

   <% @occupations.each do |occupation| %>
        <tr>
            <td><%= occupation.title%></td>
            <td><%= link_to 'Show', occupation_path(occupation) %></td>
            <td><%= link_to 'Edit', edit_occupation_path(occupation) %></td>
            <td><%= link_to 'Delete', occupation_path(occupation),
              method: :delete,
              data: {confirm: 'Are yuh sure ?'}%></td>
        </tr>
    <% end %>
</table>

<%= link_to 'New', new_occupation_path %>
<%= link_to 'Books', books_path %> <br>
<%= link_to 'index', users_path %> <br>
