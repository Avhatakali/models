<h4>house details</h4>


<%= link_to 'new', new_house_path %>

<table>
    <tr>
       <th>City</th>
       <th>Number of rooms</th>
       <th>Has garage</th>
    </tr>
 <% @houses.each do |house|%>
     <tr>
        <td><%= house.city %></td>
        <td><%= house.number_of_rooms %></td>
        <td><%= house.has_garage %></td>
        <td><%= link_to 'Show', house_path(house) %></td>
        <td><%= link_to 'Edit', edit_house_path(house) %></td>
        <td><%= link_to 'Delete', house_path(house),
          method: :delete,
          data: {confirm: 'Are yuh sure ?'} %></td>
     </tr>
 <% end %>

</table>

