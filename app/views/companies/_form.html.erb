<%= form_with(model: company, local: true) do |form| %>

      <p>
            <%= form.label :Name %> <br>
            <%= form.text_field :name %>
      </p>


       <p>
            <%= form.label :Industry %> <br>
            <%= form.text_field :industry %>
       </p>

       <p>
            <%= form.submit %>
       </p>
<% end %>
